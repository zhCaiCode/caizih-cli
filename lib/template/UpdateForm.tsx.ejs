<%_ if (data) { _%>
import React from 'react';
import { Modal } from 'antd';
import { ProForm, ProFormDigit, ProFormText, ProFormTextArea } from '@ant-design/pro-components';
import { FormattedMessage } from '@umijs/max';
import { use<%= data.name %>Updateform, UpdateFormProps } from '../../hooks/use<%= data.name %>Updateform';


const UpdateForm: React.FC<UpdateFormProps> = (props) => {

    const {
        updateModalVisible,
        onCancel,
        onSubmit,
        values,
        handleSubmit,
        form
    } = use<%= data.name %>Updateform(props)


    return (
        <Modal
            title={values?.id ? '编辑' : '新增'}
            open={updateModalVisible}
            onOk={handleSubmit}
            onCancel={() => {
                onCancel?.()
            }}
            destroyOnClose
            width={1100}
        >
            <ProForm
                grid={false}
                form={form}
                layout="horizontal"
                submitter={false}
                onFinish={onSubmit}>

                <ProFormText
                    name="name"
                    label="名称"
                    placeholder="请输入名称"
                    colProps={{ xs: 24, md: 12, xl: 12 }}
                    rules={[
                        {
                            required: true,
                            message: (
                                <FormattedMessage id="请输入名称！" defaultMessage="请输入名称！" />
                            ),
                        },
                    ]}
                />
                <ProFormText
                    name="code"
                    label="编号"
                    placeholder="请输入编号"
                    colProps={{ xs: 24, md: 12, xl: 12 }}
                    rules={[
                        {
                            required: true,
                            message: (
                                <FormattedMessage id="请输入编号！" defaultMessage="请输入编号！" />
                            ),
                        },
                    ]}
                />
                <ProFormDigit
                    name="count"
                    label="数量"
                    placeholder="请输入数量"
                    width="sm"
                    min={1}
                    fieldProps={{
                        controls: false
                    }}
                    rules={[
                        {
                            required: true,
                            message: (
                                <FormattedMessage id="请输入数量！" defaultMessage="请输入数量！" />
                            ),
                        },
                    ]}
                />
                <ProFormTextArea
                    name="remark"
                    label="描述说明"
                    placeholder="请输入描述说明"
                    colProps={{ xs: 24, md: 12, xl: 12 }}
                />
            </ProForm>
        </Modal>
    );
};

export default UpdateForm;
<%_ } _%>