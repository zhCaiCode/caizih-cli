<% if (data) { %>
    import request from '@/utils/request'

    // 查询列表
    export function list<%= data.name %>(query) {
      return request({
        url: '/system/<%= data.lowerName %>/list',
        method: 'get',
        params: query
      })
    }
    
    // 查询详细
    export function get<%= data.name %>(id) {
      return request({
        url: '/system/<%= data.lowerName %>/' + id,
        method: 'get'
      })
    }
    
    // 新增记录
    export function add<%= data.name %>(data) {
      return request({
        url: '/system/<%= data.lowerName %>',
        method: 'post',
        data: data
      })
    }
    
    // 修改记录
    export function update<%= data.name %>(data) {
      return request({
        url: '/system/<%= data.lowerName %>',
        method: 'put',
        data: data
      })
    }
    
    // 删除记录
    export function del<%= data.name %>(id) {
      return request({
        url: '/system/<%= data.lowerName %>/' + id,
        method: 'delete'
      })
    }    
<% } %>