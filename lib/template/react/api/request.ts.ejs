<%_ if (data) { _%>
  import request from '@/utils/request'

  // 查询
  export async function get<%= data.name %>List(params?: any) {
    return request<any>('/api/system/<%= data.lowerName%>/list', {
      method: 'GET',
      params
    })
  }
  export async function getAll<%= data.name %>List(params?: any) {
    return request<any>('/api/system/<%= data.lowerName%>/array', {
      method: 'GET',
      params
    })
  }
  // 获取详情
  export async function get<%= data.name %>Detail(id?: number) {
    return request(`/api/system/<%= data.lowerName%>/${id}`, {
      method: 'GET'
    })
  }
  
  // 新增
  export async function add<%= data.name %>(data: any) {
    return request('/api/system/<%= data.lowerName%>', {
      method: 'POST',
      data
    })
  }
  
  // 编辑
  export async function update<%= data.name %>(data: any) {
    return request('/api/system/<%= data.lowerName%>', {
      method: 'PUT',
      data
    })
  }
  // 删除
  export async function remove<%= data.name %>(ids: (number | string)[]) {
    return request(`/api/system/<%= data.lowerName%>/${ids}`, {
      method: 'DELETE'
    })
  }
<%_ } _%>