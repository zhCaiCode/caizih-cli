<%_ if (data) { _%>
import { get<%= data.name %>List, remove<%= data.name %> } from '@/services/moduleName/<%= data.lowerName %>'
import { ActionType } from '@ant-design/pro-components'
import { message } from 'antd'
import type { <%= data.name %>Handlers } from 'types/moduleName'
export const use<%= data.name %>Data = (actionRef: React.MutableRefObject<ActionType | undefined>): <%= data.name %>Handlers => {
  const handleRemove = async (selectedRows: any[]): Promise<void> => {
    if (!selectedRows) return
    try {
      const ids: number[] = selectedRows.map(row => row.id)
      const { code, msg } = await remove<%= data.name %>(ids)
      if (code !== 200) {
        throw new Error(msg)
      }
      message.success('删除成功')
      actionRef.current?.reload()
    } catch (error) {
      message.error('删除失败，请重试')
      console.error('删除失败，请重试', error)
    }
  }

  const fetchTypeList = async (params: any) => {
    const response = await get<%= data.name %>List({
      ...params,
      pageNum: params.current,
      pageSize: params.pageSize
    })
    return {
      data: response.rows,
      total: response.total,
      success: true
    }
  }

  return {
    handleRemove,
    fetchTypeList
  }
}


    
    
    
    
